{
  "name": "bootstrap_handson",
  "version": "1.0.0",
  "description": "coursera bootstrap learning course ",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:para-all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "live": "live-server --port=9000",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:para-all": "parallelshell \"npm run watch:scss\" \"npm run live\"",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "imgemin": "imagemin img/* -o dist/img",
    "usemin": "usemin contactus.html -d dist --htmlmin -o dist/contactus.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "build": "npm run clean && npm run copyfonts && npm run imgemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/x-claimer/Bootstrap.git"
  },
  "author": "Mohammed Ateeq Ur Rehman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/x-claimer/Bootstrap/issues"
  },
  "homepage": "https://github.com/x-claimer/Bootstrap#readme",
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "cssmin": "^0.4.3",
    "grunt": "^1.3.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-htmlmin": "^3.1.0",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^3.1.0",
    "grunt-usemin": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.1.0",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "parallelshell": "^3.0.1",
    "rimraf": "^3.0.2",
    "time-grunt": "^2.0.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "less": "^3.12.2",
    "mdbootstrap": "^4.19.1",
    "popper.js": "^1.16.1"
  }
}
